{
  "name": "vscode-sanity",
  "version": "0.0.3",
  "license": "MIT",
  "displayName": "Sanity.io",
  "description": "Developer tools for applications powered by Sanity.io",
  "author": "Sanity.io <hello@sanity.io>",
  "publisher": "sanity-io",
  "engines": {
    "vscode": "^1.29.0"
  },
  "keywords": [
    "vscode",
    "visual studio code",
    "sanity",
    "groq"
  ],
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sanity-io/vscode-sanity.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/vscode-sanity/issues"
  },
  "homepage": "https://github.com/sanity-io/vscode-sanity/blob/master/README.md",
  "icon": "images/icon.png",
  "contributes": {
    "languages": [
      {
        "id": "groq",
        "aliases": [
          "GROQ",
          "groq"
        ],
        "extensions": [
          ".groq"
        ],
        "configuration": "./language/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "groq",
        "scopeName": "source.groq",
        "path": "./grammars/groq.json"
      },
      {
        "injectTo": [
          "source.js",
          "source.ts",
          "source.js.jsx",
          "source.tsx",
          "source.vue",
          "source.svelte",
          "source.php"
        ],
        "scopeName": "inline.groq",
        "path": "./grammars/groq.js.json",
        "embeddedLanguages": {
          "meta.embedded.block.groq": "groq"
        }
      },
      {
        "scopeName": "markdown.groq.codeblock",
        "path": "./grammars/groq.md.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.groq": "javascript"
        }
      }
    ]
  },
  "dependencies": {
    "typescript": "^4.0.3",
    "vsce": "^1.81.1",
    "vscode": "^1.1.37"
  },
  "devDependencies": {
    "prettier": "^2.1.2"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "vsce:package": "vsce package",
    "env:source": "export $(cat .envrc | xargs)",
    "vsce:publish": "sh publish.sh",
    "upgrade-interactive": "npx npm-check -u"
  }
}
